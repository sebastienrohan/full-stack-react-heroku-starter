{
  "name": "full-stack-react-starter",
  "version": "1.0.0",
  "description": "Based on create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "6.9.x"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd react-ui/ && npm install --only=dev && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.14.1",
    "jsonwebtoken": "^7.4.1",
    "mongoose": "^4.10.8",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "validator": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastienrohan/full-stack-react-heroku-starter"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "dotenv": "^4.0.0"
  }
}
